{
  "name": "family-docs",
  "version": "1.0.0",
  "main": "src/index.ts",
  "author": "Jose Hower",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.ts --bundle --outfile=dist/family-docs.mjs --platform=node --target=node16.8 --format=esm --banner:js=\"import { createRequire as topLevelCreateRequire } from 'module'; const require = topLevelCreateRequire(import.meta.url); import { dirname } from 'node:path'; import { fileURLToPath } from 'node:url'; const __filename = fileURLToPath(import.meta.url); const __dirname = dirname(__filename); \"",
    "dev": "nodemon --watch '*/**.ts' -e ts --exec 'yarn build && node dist/family-docs.mjs'",
    "start": "node dist/family-docs.mjs"
  },
  "dependencies": {
    "@fastify/cookie": "^7.0.0",
    "@next/eslint-plugin-next": "12.1.6",
    "dotenv-safe": "^8.2.0",
    "esbuild": "0.14.42",
    "fastify": "^4.1.0",
    "firebase-admin": "^11.0.0",
    "nodemon": "^2.0.19",
    "typescript": "4.7.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.2",
    "@types/node": "17.0.41",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "@upleveled/eslint-config-upleveled": "2.12.0",
    "@upleveled/eslint-plugin-upleveled": "2.1.5",
    "eslint": "8.17.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-security": "1.5.0",
    "eslint-plugin-sonarjs": "0.13.0",
    "eslint-plugin-testing-library": "5.5.1",
    "eslint-plugin-unicorn": "42.0.0"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.30.0",
    "eslint-plugin-react-hooks": "4.5.0",
    "eslint-plugin-jest": "26.5.3",
    "@typescript-eslint/utils": "5.27.1"
  }
}
